inp w     # w = 14th digit
mul x 0   # x = 0
add x z   # x = z [result of 13th digit]
mod x 26  # x = z%26
div z 26  # z = z/26
add x -5  # x = (z%26) -5
eql x w   # is 14thd digit (z%26)-5?
eql x 0   # x = !x
mul y 0
add y 25  # y = 25
mul y x 
add y 1
mul z y
mul y 0
add y w
add y 9
mul y x
add z y
